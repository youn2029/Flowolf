package kr.or.dev.talk.chat_file.model;

import java.util.Date;

public class ChatFileVO {
	
	private int chat_file_no;			// 채팅첨부파일번호
	private Date chat_file_time;		// 등록일시
	private String chat_file_name;		// 파일명
	private String chat_file_path;		// 파일경로
	private String chat_file_upload;	// 업로드파일명	
	private int chat_no;				// 채팅방번호

	public int getChat_file_no() {
		return chat_file_no;
	}

	public void setChat_file_no(int chat_file_no) {
		this.chat_file_no = chat_file_no;
	}

	public Date getChat_file_time() {
		return chat_file_time;
	}

	public void setChat_file_time(Date chat_file_time) {
		this.chat_file_time = chat_file_time;
	}

	public String getChat_file_name() {
		return chat_file_name;
	}

	public void setChat_file_name(String chat_file_name) {
		this.chat_file_name = chat_file_name;
	}

	public String getChat_file_path() {
		return chat_file_path;
	}

	public void setChat_file_path(String chat_file_path) {
		this.chat_file_path = chat_file_path;
	}

	public String getChat_file_upload() {
		return chat_file_upload;
	}

	public void setChat_file_upload(String chat_file_upload) {
		this.chat_file_upload = chat_file_upload;
	}

	public int getChat_no() {
		return chat_no;
	}

	public void setChat_no(int chat_no) {
		this.chat_no = chat_no;
	}

	
}
