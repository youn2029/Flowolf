package kr.or.dev.timeline.emoticon.model;

import java.util.Date;

public class EmoticonVO {

	private int emo_no;					// 이모티콘번호(pk)
	private String emo_name;			// 이모티콘명
	private String emo_pic_name;		// 이모티콘파일명
	private String emo_pic_upload;		// 이모티콘업로드명
	private String emo_pic_path;		// 이모티콘경로
	private Date emo_date;				// 이모티콘등록일	
	private String emo_del_chk;			// 이모티콘사용유무

	public int getEmo_no() {
		return emo_no;
	}

	public void setEmo_no(int emo_no) {
		this.emo_no = emo_no;
	}

	public String getEmo_name() {
		return emo_name;
	}

	public void setEmo_name(String emo_name) {
		this.emo_name = emo_name;
	}

	public String getEmo_pic_name() {
		return emo_pic_name;
	}

	public void setEmo_pic_name(String emo_pic_name) {
		this.emo_pic_name = emo_pic_name;
	}

	public String getEmo_pic_upload() {
		return emo_pic_upload;
	}

	public void setEmo_pic_upload(String emo_pic_upload) {
		this.emo_pic_upload = emo_pic_upload;
	}

	public String getEmo_pic_path() {
		return emo_pic_path;
	}

	public void setEmo_pic_path(String emo_pic_path) {
		this.emo_pic_path = emo_pic_path;
	}

	public Date getEmo_date() {
		return emo_date;
	}

	public void setEmo_date(Date emo_date) {
		this.emo_date = emo_date;
	}

	public String getEmo_del_chk() {
		return emo_del_chk;
	}

	public void setEmo_del_chk(String emo_del_chk) {
		this.emo_del_chk = emo_del_chk;
	}
	
}
